{
    "name": "node-express-sequelize",
    "version": "1.0.0",
    "description": "A boilerplate setup for RESTful API's using node, express & sequelize",
    "main": "src/index.js",
    "scripts": {
        "build": "tsc",
        "start:dev": "cross-env NODE_ENV=development nodemon --exec ts-node src/index.ts",
        "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prettier": "prettier --check **/*.ts",
        "prettier:fix": "prettier --write **/*.ts **/*/*.ts",
        "start": "ts-node-dev --respawn src/index.ts",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Jothinayagan/node-express-sequelize.git"
    },
    "keywords": [
        "node",
        "express",
        "sequelize",
        "sql-orm"
    ],
    "author": "Jothinayagan",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Jothinayagan/node-express-sequelize/issues"
    },
    "homepage": "https://github.com/Jothinayagan/node-express-sequelize#readme",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "http-status": "^1.5.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "mysql2": "^2.3.3",
        "sequelize": "^6.16.1",
        "validator": "^13.7.0",
        "winston": "^3.6.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node": "^17.0.18",
        "eslint": "^8.9.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-security": "^1.4.0",
        "nodemon": "^2.0.15",
        "prettier": "^2.5.1",
        "ts-node": "^10.5.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.5.5"
    }
}
